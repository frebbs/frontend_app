{
  "name": "website_frontend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "script:tree": "tree -a -I 'node_modules|.idea|.git|.vscode'",
    "start:prod": "node server.js",
    "start:dev": "nodemon server.js",
    "start:browserSync": "browser-sync start --proxy \"localhost:5000\" --files \"views/**/*.njk, public/css/*.css, config/*.json, public/js/*.js\"",
    "start:js": "webpack --watch --mode development",
    "sass:watch": "sass --watch src/scss/main.scss:public/css/main.css",
    "dev": "concurrently \"npm run start:dev\" \"npm run sass:watch\" \"npm run start:js\"",
    "build:js": "webpack --mode production",
    "build:sass": "sass src/scss/main.scss:public/css/main.css",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "lint:js": "eslint --fix .",
    "test": "echo 'Tests Ran!'"
  },
  "lint-staged": {
    "*.js": "npm run lint:js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.4.4",
    "nunjucks": "^3.2.4",
    "sass": "^1.77.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
